@model IEnumerable<ScheduleProg.Models.PareSubgroup>

@{
    ViewData["Title"] = "AdminView";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Пара
            </th>
            <th>
               Група
            </th>
             
            
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pare.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subgroup.Id)
            </td>
             
        
            <td>
                 <a asp-action="Delete" asp-route-id="@item.Pare.Id">Видалити</a>
            </td>
        </tr>
}
    </tbody>
</table>

